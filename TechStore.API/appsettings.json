{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=DB/TechStore.db"
  },
  "ImageService": {
    "ProductImagesPath": "ProductImages",
    "MaxFileSize": 1048576,
    "MaxLargestImageSide": 2048,
    "DefaultLargestImageSide": 512,
    "AllowedContentTypes": [ "image/png", "image/jpeg" ]
  },
  "ShopProfile": {
    "ProductImagesEndpoint": "http://localhost:5260/ProductImage"
  },
  "AllowedHosts": "*"
}
